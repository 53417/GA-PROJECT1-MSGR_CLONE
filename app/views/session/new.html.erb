<p>Find me in app/views/session/new.html.erb</p>

<h1>Login</h1>
<%= form_with(url: "/login", method: "post") do %>
  <div>
    <label>
      Username
      <input type="string" name="username"/>
    </label>
  </div>
  <div>
    <label>
      Password
      <input type="password" name="password"/>
    </label>
    <div>
      <input type="submit" value="Login"/>
    </div>
  </div>
<% end %>
<div>
  <%= @error_message %>
</div>